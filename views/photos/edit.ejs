
<% include ../partials/header %>
<h1>Edit form</h1>

<h1>Edit <%= photo.name %></h1>

  <form action="/photos/<%= photo._id %>/edit?_method" method="POST">
    <div class="form-group">
      <input type="text" name="comment[text]" class="form-control" placeholder="Text">
    </div>
    <div class="form-group">
      <input type="text" name="comment[author]" class="form-control" placeholder="Author">
    </div>
    <p>
      <button class="btn btn-primary">Submit</button>
    </p>
  </form>
<% include ../partials/footer %>
