<% include ../partials/header %>

<div class="container">
<form action="/photos" method="POST">
  <div class="form-group">
    <label for="nameOfPhoto">Name of Photo</label>
    <input type="text" name="name" class="form-control" id="nameOfPhoto" placeholder="Name of Photo">
  </div>
  <div class="form-group">
    <label for="imageUrl">Image URL</label>
    <input type="text" name="image" class="form-control" id="imageUrl" placeholder="Image URL">
  </div>
  <div class="form-group">
    <label for="description">Description</label>
    <input type="text" name="description" class="form-control" id="description" placeholder="Description">
  </div>
  <p>
    <button class="btn btn-primary">Submit</button>
  </p>
</form>
</div>
<% include ../partials/footer %>
