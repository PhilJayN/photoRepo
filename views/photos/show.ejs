<% include ../partials/header %>

<h1>show pg!</h1>

<p>
  <%= photo.name %>
</p>

<p>
  <img src="  <%= photo.image %> " alt="" />
</p>

<p>
  <%= photo.description %>
</p>

<p>
  <a href="/photos/<%= photo._id %>/comments/new">Add a comment</a>
</p>

<p>
  User commments:
</p>

<p>
  <% photo.comments.forEach(function(comment){ %>
    <% console.log(comment) %>

    <p>
      <%= comment.text %>
    </p>
  <% }) %>
</p>

<% include ../partials/footer %>
